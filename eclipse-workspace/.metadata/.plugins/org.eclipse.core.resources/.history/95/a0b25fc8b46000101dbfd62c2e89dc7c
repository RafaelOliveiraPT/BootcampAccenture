package gestorstocks;

import java.util.ArrayList;

public class SampleData {	
	
	public static ArrayList<Produto> getSampleDataProducts(){
		ArrayList<Produto> dataSampleProductsList = new ArrayList<Produto>(); 
		// dados para teste
		Produto a1 = new Produto();
		a1.setDiscount(2);
		a1.setIva(23);
		a1.setPvp(22);
		a1.setName("Sapato");
		dataSampleProductsList.add(a1);
		
		Produto a2 = new Produto();
		a2.setDiscount(2);
		a2.setIva(23);
		a2.setPvp(22);
		a2.setName("Camisola");
		dataSampleProductsList.add(a2);
		
		Produto a3 = new Produto();
		a3.setDiscount(5);
		a3.setIva(23);
		a3.setPvp(40);
		a3.setName("Calças");
		dataSampleProductsList.add(a3);

		Produto a4 = new Produto();
		a4.setDiscount(10);
		a4.setIva(23);
		a4.setPvp(15);
		a4.setName("Boné");
		dataSampleProductsList.add(a4);

		Produto a5 = new Produto();
		a5.setDiscount(0);
		a5.setIva(23);
		a5.setPvp(50);
		a5.setName("Relógio");
		dataSampleProductsList.add(a5);
		
		return dataSampleProductsList;
	}
	
	public static ArrayList<Prateleira> getSampleDataShelves(){
		ArrayList<Prateleira> dataSampleShelvesList = new ArrayList<Prateleira>();
		Prateleira p1 = new Prateleira();
		p1.setCapacity(1);
		p1.setDiariaAluguer(.20);
		dataSampleShelvesList.add(p1);
		Prateleira p2 = new Prateleira();
		p2.setCapacity(1);
		p2.setDiariaAluguer(0.30);
		dataSampleShelvesList.add(p2);

		Prateleira p3 = new Prateleira();
		p3.setCapacity(1);
		p3.setDiariaAluguer(0.40);
		dataSampleShelvesList.add(p3);

		Prateleira p4 = new Prateleira();
		p4.setCapacity(1);
		p4.setDiariaAluguer(0.25);
		dataSampleShelvesList.add(p4);

		Prateleira p5 = new Prateleira();
		p5.setCapacity(1);
		p5.setDiariaAluguer(0.50);
		dataSampleShelvesList.add(p5);
		
		return dataSampleShelvesList;
	}
